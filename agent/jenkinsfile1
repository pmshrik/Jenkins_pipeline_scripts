pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                withCredentials([usernamePassword(credentialsId: 'test_id', usernameVariable: 'USER', passwordVariable: 'PASS')]){
                echo "$USER $PASS"
                sh '''
                    $USER $PASS
                '''
                }
            }
        }

    }
}